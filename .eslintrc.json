{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    // "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": 0,
    "no-use-before-define": 0,
    "func-names": 0,
    "object-shorthand": 0,
    "class-methods-use-this": 0,
    "import/prefer-default-export": 0,
    "one-var": 0,
    "prefer-const": 0,
    "no-param-reassign": 0
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  }
}
